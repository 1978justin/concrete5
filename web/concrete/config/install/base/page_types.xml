<?xml version="1.0"?>
<concrete5-cif version="1.0">

    <pagetypes>
        <pagetype handle="blog_entry" name="Blog Entry" package="">
            <composer method="PAGE_TYPE" pagetype="{ccm:export:pagetype:landing}" parent="">
                <items>
                    <attributekey handle="tags"/>
                    <block type="content" name="Content"/>
                </items>
            </composer>
            <page name="" path="" filename="" pagetype="blog_entry" description="" package="">
                <area name="Blog Post Header"/>
                <area name="Main">
                    <block type="content" name="Content">
                        <data table="btContentLocal">
                            <record>
                                <content><![CDATA[]]></content>
                            </record>
                        </data>
                    </block>
                </area>
                <area name="Blog Post More"/>
                <area name="Blog Post Footer"/>
                <area name="Header Nav"/>
                <area name="Header"/>
                <area name="Sidebar"/>
            </page>
        </pagetype>
        <pagetype handle="landing" name="Landing" package="">
            <page name="" path="" filename="" pagetype="landing" description="" package=""/>
        </pagetype>
        <pagetype handle="page" name="Page" package="">
            <page name="" path="" filename="" pagetype="page" description="" package="">
                <area name="Header Nav">
                    <block type="autonav" name="" custom-template="header_menu.php">
                        <data table="btNavigation">
                            <record>
                                <orderBy><![CDATA[display_asc]]></orderBy>
                                <displayPages><![CDATA[top]]></displayPages>
                                <displayPagesCID/>
                                <displayPagesIncludeSelf><![CDATA[0]]></displayPagesIncludeSelf>
                                <displaySubPages><![CDATA[none]]></displaySubPages>
                                <displaySubPageLevels><![CDATA[enough]]></displaySubPageLevels>
                                <displaySubPageLevelsNum><![CDATA[0]]></displaySubPageLevelsNum>
                                <displayUnavailablePages><![CDATA[0]]></displayUnavailablePages>
                            </record>
                        </data>
                    </block>
                </area>
                <area name="Header">
                    <block type="content" name="">
                        <data table="btContentLocal">
                            <record>
                                <content><![CDATA[<p>Header</p>]]></content>
                            </record>
                        </data>
                    </block>
                </area>
                <area name="Sidebar">
                    <block type="autonav" name="">
                        <data table="btNavigation">
                            <record>
                                <orderBy><![CDATA[display_asc]]></orderBy>
                                <displayPages><![CDATA[current]]></displayPages>
                                <displayPagesCID/>
                                <displayPagesIncludeSelf><![CDATA[0]]></displayPagesIncludeSelf>
                                <displaySubPages><![CDATA[none]]></displaySubPages>
                                <displaySubPageLevels><![CDATA[enough]]></displaySubPageLevels>
                                <displaySubPageLevelsNum><![CDATA[0]]></displaySubPageLevelsNum>
                                <displayUnavailablePages><![CDATA[0]]></displayUnavailablePages>
                            </record>
                        </data>
                    </block>
                </area>
            </page>
        </pagetype>
    </pagetypes>

</concrete5-cif>
