<?xml version="1.0"?>
<concrete5-cif version="1.0">
    <aggregatorsources>
        <aggregatorsource handle="page" name="Site Page" package="" />
        <aggregatorsource handle="rss_feed" name="RSS Feed" package="" />
    </aggregatorsources>
    <features>
        <feature handle="title" score="1" />
        <feature handle="link" score="1" />
        <feature handle="date_time" score="1" />
        <feature handle="image" score="10" has-custom-class="true" />
        <feature handle="conversation" score="10" has-custom-class="true" />
        <feature handle="description" score="1" />
        <feature handle="featured" score="20" />
    </features>
    <aggregatoritemtemplates>
        <aggregatoritemtemplate handle="featured" name="Featured Item" package="" fixed-slot-width="6" fixed-slot-height="2" force-default="1">
            <feature handle="description" />
            <feature handle="date_time" />
            <feature handle="link" />
            <feature handle="title" />
            <feature handle="image" />
            <feature handle="featured" />
        </aggregatoritemtemplate>
        <aggregatoritemtemplate handle="title_description" name="Title Date &amp; Description" package="">
            <feature handle="description" />
            <feature handle="date_time" />
            <feature handle="link" />
            <feature handle="title" />
        </aggregatoritemtemplate>
        <aggregatoritemtemplate handle="title_date" name="Title &amp; Date" package="">
            <feature handle="date_time" />
            <feature handle="link" />
            <feature handle="title" />
        </aggregatoritemtemplate>
        <aggregatoritemtemplate handle="title_date_comments" name="Title, Date &amp; Comments" package="" has-custom-class="true">
            <feature handle="date_time" />
            <feature handle="link" />
            <feature handle="title" />
            <feature handle="conversation" />
        </aggregatoritemtemplate>
        <aggregatoritemtemplate handle="thumbnail" name="Thumbnail Only" package="">
            <feature handle="link" />
            <feature handle="image" />
        </aggregatoritemtemplate>
    </aggregatoritemtemplates>
    <featurecategories>
        <featurecategory handle="collection_version" />
        <featurecategory handle="aggregator_item" />
    </featurecategories>
</concrete5-cif>
