<doctrine-mapping>
  <entity table="BlockTypes" name="Concrete\Core\Block\BlockType\BlockType">
    <id name="btID" type="integer">
        <generator strategy="AUTO" />
      </id>
    <field name="btHandle" length="128" />
    <field name="btName" length="128" />
    <field name="btDescription" type="text" />
    <field name="btCopyWhenPropagate" type="boolean" />
    <field name="btIncludeAll" type="boolean" />
    <field name="btIsInternal" type="boolean" />
    <field name="btSupportsInlineAdd" type="boolean" />
    <field name="btSupportsInlineEdit" type="boolean" />
    <field name="btInterfaceWidth" type="integer" />
    <field name="btInterfaceHeight" type="integer" />
    <field name="pkgID" type="integer" />
  </entity>
</doctrine-mapping>